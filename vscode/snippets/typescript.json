{
	/*
	// Place your snippets for TypeScript here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:*/
	"Print to console": {
		"prefix": "log",
		"body": [
			"winston.debug('$1');",
			"$2"
		],
		"description": "Log output to console"
	},
	"No parameters callback": {
		"prefix": "cb",
		"body": [
			"() => {",
			"	$0",
			"}"
		],
		"description": "Callback with no parameters"
	},
	"Result parameter callback": {
		"prefix": "cr",
		"body": [
			"(${1:result}) => {",
			"	$0",
			"}"
		],
		"description": "Callback with a parameter"
	},
	"Result with two parameter callback": {
		"prefix": "crr",
		"body": [
			"(${1:a}, ${2:b}) => {",
			"	$0",
			"}"
		],
		"description": "Callback with a parameter"
	},
	"Request Response": {
		"prefix": "rest",
		"body": [
			"(req: Request | any, resp: Response) => {",
			"	$0",
			"}"
		],
		"description": "Callback with a parameter"
	},
	"REST get": {
		"prefix": "rget",
		"body": [
			"router.get('/$1', (req: Request | any, res: Response) => {",
			"	$0",
			"})"
		]
	},
	"REST post": {
		"prefix": "rpost",
		"body": [
			"router.post('/$1', (req: Request | any, res: Response) => {",
			"	$0",
			"})"
		]
	},
	"REST put": {
		"prefix": "rput",
		"body": [
			"router.put('/$1', (req: Request | any, res: Response) => {",
			"	$0",
			"})"
		]
	},
	"REST delete": {
		"prefix": "rdel",
		"body": [
			"router.delete('/$1', (req: Request | any, res: Response) => {"
			"	$0",
			"})"
		]
	},
	"Import": {
		"prefix": "imfrom"
		"body": [
			"import { $2 } from '$1'"
		],
		"description": "Import from lib"
	},
	"Import as": {
		"prefix": "imall",
		"body": "import * as $1 from '$2'$0",
		"description": "Import from require javascript"
	},
	"Console log": {
		"prefix": "clog",
		"body": [
			"console.log($1);",
			"$0"
		],
		"description": "Console log"
	}

}